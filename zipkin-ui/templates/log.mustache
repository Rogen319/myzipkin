<div class="row well well-sm">
  <form id="dependency-query-form" class="form-inline" role="form">
    <div id="start-ts" class="form-group">
      <label for="timeStamp" data-i18n="dep.stime">Start time</label>
      <input class="form-control input-sm date-input" type="text">
      <input class="form-control input-sm time-input" type="text">
      <input class="timestamp-value" id="startTs" name="startTs" type="hidden" value="">
    </div>

    <div id="end-ts" class="form-group">
      <label for="timeStamp" data-i18n="dep.etime">End time</label>
      <input class="form-control input-sm date-input" type="text">
      <input class="form-control input-sm time-input" type="text">
      <input class="timestamp-value" id="endTs" name="endTs" type="hidden" value="">
    </div>

    <button type="submit" class="btn btn-default btn-sm" data-i18n="dep.analyze">Analyze Dependencies</button>

    <div class="form-group">
      <label for="classifyNumber">分类数量：</label>
      <input type="number" id = "classifyNumber" min="2" max="10"/>
    </div>
    <button class="btn btn-default btn-sm" id="classify">分类</button>

  </form>
</div>

<div style="display: inline">
  <button class="btn btn-default btn-sm pull-right" id="controlLogVis" style="display: none;margin-right: -15px;margin-top: -20px;"></button>
  <button class="btn btn-default btn-sm pull-right" id="asyncSequenceButton" style="display: none;margin-top: -20px" data-toggle="modal" data-target="#asyncModal">查看异步调用</button>
</div>

<div class="modal fade" id="asyncModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" >
  <div class="modal-dialog" style="width: 1400px">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
          &times;
        </button>
        <h4 class="modal-title" id="myModalLabel">
          AsyncSequences
        </h4>
      </div>
      <div class="modal-body" id="asyncModalBody">
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">close
        </button>
      </div>
    </div><!-- /.modal-content -->
  </div><!-- /.modal -->
</div>


<div class="row">
  <div id="dependency-container" class="col-sm-12 col-md-12 col-lg-12">
    <ul class="list-group pull-right selected-trace" id = "selectedTrace">
    </ul>
    <svg width="100%" height="4000" class="rootSvg">
    </svg>
  </div>
</div>

<div id="selectTree"></div>

<div id="controllTree">
  <button id="controllTreeBtn" class="btn btn-default btn-xs diybtn">隐藏调用链菜单</button>
</div>


